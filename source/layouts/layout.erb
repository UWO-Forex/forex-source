    <!-- Fonts-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,300,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href="../stylesheets/font-awesome.min.css" rel="stylesheet">

    <!-- Bootstrap -->
    <link href="../stylesheets/bootstrap.min.css" rel="stylesheet">
    <link href="../stylesheets/owl.carousel.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="../stylesheets/preloader.css" />
    <link href="../stylesheets/style.css" rel="stylesheet">
    <link href="../stylesheets/responsive.css" rel="stylesheet">
    <script src="../javascripts/modernizr.custom.js"></script>
  </head>



  <body class="<%= page_classes %>" id="bigWrapper" data-spy="scroll" data-target=".navbar-default" data-offset="100">
  <header>
  <nav class="navbar navbar-default navbar-fixed-top" role="navigation" data-spy="affix" data-offset-top="0">
    <div class="container-fluid">
      <!-- Brand and toggle get grouped for better mobile display -->
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">WESTERN FOREX</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse navbar-right" id="navbar-collapse">
        <ul class="nav navbar-nav">
          <li><a data-scroll href="../.">Home</a></li>
          <% if current_page.data.page_flag == 'about' %>
              <li class="active"><a data-scroll href="../about">About</a></li>
              <% else %><li><a data-scroll href="../about">About</a></li>
          <% end %>
          <% if current_page.data.page_flag == 'executives' %>
              <li class="active"><a data-scroll href="../executives">Executives</a></li>
          <% else %><li><a data-scroll href="../executives">Executives</a></li>
          <% end %>
          <% if current_page.data.page_flag == 'events' %>
              <li class="active"><a data-scroll href="../events">Events</a></li>
          <% else %><li><a data-scroll href="../events">Events</a></li>
          <% end %>
          <% if current_page.data.page_flag == 'handbook' %>
              <li class="active"><a data-scroll href="../handbook">Handbook</a></li>
          <% else %><li><a data-scroll href="../handbook">Handbook</a></li>
          <% end %>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
  </nav>
  </header>
    <%= yield %>
  <section id="footer" class="dark">
    <div class="bottomLine text-center">
      <ul class="footerSocialIcons">
        <li><a href="https://www.facebook.com/westernforex"><i class="fa fa-facebook-square fa-3x"></i></a></li>
        <li><a href="https://www.linkedin.com/company/western-forex-association"><i class="fa fa-linkedin-square fa-3x"></i></a></li>
      </ul>
      <p>Site Design by <b>Scott Mackie</b> for the Western Forex Association - 2015</p>
    </div>
  </section>



  <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
  <script type="text/javascript" src="../javascripts/scripts.js"></script>
  <script src="../javascripts/preloader.js"></script>
  <script src="../javascripts/custom.js"></script>

  </body>
</html>
